{
    "autoload": {
        "classmap": ["src/"],
        "psr-4": {
            "PackageName\\": "src",
            "PackageName\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "test": [
            "composer dump-autoload",
            "./vendor/bin/phpunit ./tests"
        ]
    }
}
